pipeline {
     agent any
     stages {
        stage("Deploy") {
            steps {
                bat "rmdir /s /q D:\\tomcat9\\webapps\\ROOT"
                bat "mkdir D:\\tomcat9\\webapps\\ROOT"
                bat "robocopy -r ${WORKSPACE}\\build\\ D:\\tomcat9\\webapps\\ROOT"
            }
        }
    }
}
